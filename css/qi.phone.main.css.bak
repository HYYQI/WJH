@import url("qi.phone.style.css");

body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 20px;
    background-color: #f4f4f9;
}

h1, h2, h3 {
    color: #333;
}

.bg_gray {
    display: inline;
    background-color: rgba(139, 139, 139);
}

.flex {
    display: flex;
}

.center {
    text-align: center;
}

.shadow {
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}

